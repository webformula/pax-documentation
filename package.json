{
  "name": "@webformula/pax-documentation",
  "version": "0.0.1",
  "description": "PAX pages and components tool set documentation",
  "main": "server.js",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/webformula/pax-documentation.git"
  },
  "scripts": {
    "start": "nodemon --watch app --watch src-docs --exec \"npm run build\" & npm run serve",
    "serve": "http-server dist/ -p 8080",
    "build": "node --experimental-modules app/client.js && cp -r app/public/* dist/"
  },
  "author": "Ben Rubin (B-3PO)",
  "license": "MIT",
  "dependencies": {
    "@webformula/pax-core": "github:webformula/pax-core#v0.3.1-beta",
    "express": "^4.16.4",
    "morgan": "^1.9.1"
  },
  "devDependencies": {
    "http-server": "^0.11.1",
    "nodemon": "^1.19.2"
  }
}
